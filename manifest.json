{
   "name": "Cookies CSV",
   "version": "1.0",
   "description": "List site wide cookies in CSV format",
   "manifest_version": 2,
   "permissions": ["cookies","<all_urls>","tabs"],
   "browser_action": {
      "browser_style": true,
      "default_title": "List site wide cookies",
      "default_popup": "cookies.html"
   }
}
